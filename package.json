{
	"name": "flat-art-5614",
	"version": "0.8.9",
	"private": true,
	"type": "module",
	"workspaces": [
		"packages/*",
		"apps/*"
	],
	"scripts": {
		"deploy": "wrangler deploy",
		"dev": "wrangler dev",
		"start": "wrangler dev",
		"test": "node scripts/test-bootstrap.js && vitest --run",
		"test:ui": "vitest --run -c vitest.ui.config.ts",
		"test:coverage": "node scripts/test-bootstrap.js && vitest --run --coverage",
		"test:a11y": "vitest --run -c vitest.a11y.config.ts",
		"coverage": "npm run test:coverage && node scripts/coverage-summary.js",
		"coverage:ratchet": "node scripts/coverage-ratchet.js",
		"coverage:badge": "node scripts/gen-coverage-badge.js",
		"data:dict": "node scripts/gen-data-dictionary.js",
		"demo:seed": "node scripts/demo-seed.js",
		"perf-smoke": "node scripts/perf-smoke.js",
		"typecheck": "tsc --noEmit",
		"lint": "eslint 'src/**/*.ts'",
		"format": "prettier -w .",
		"cf-typegen": "wrangler types",
		"contract": "node scripts/contract-check.js",
		"version-check": "node scripts/version-check.js",
		"storybook": "storybook dev -p 6006",
		"build:storybook": "storybook build",
		"e2e": "playwright test",
		"dev:web": "npm --workspace=@app/web run dev"
	},
	"devDependencies": {
