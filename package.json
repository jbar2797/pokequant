		"@types/react": "^18.2.66",
		"@types/react-dom": "^18.2.22",
		"@typescript-eslint/eslint-plugin": "^7.8.0",
		"@typescript-eslint/parser": "^7.8.0",
		"@tanstack/react-virtual": "^3.0.0",
		"@vitest/coverage-istanbul": "^3.2.4",
		"autoprefixer": "^10.4.19",
